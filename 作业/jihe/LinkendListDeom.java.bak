import java.util.*;
public class LinkendListDeom{
	public static void main(String [] args){
		//创建LinkendList集合
		List<String> ll = new LinkedList<String>();
		//添加元素
		ll.add("lin");
		ll.add("feng");
		ll.add("zhen");
		ll.add("hai");
		//移除指定元素
		ll.remove("feng");
		//移除所有元素
		//ll.clear();
		//输出返回指定元素
		//ll.get(1);
		System.out.println("指定元素是"+ll.get(1));
		//输出指定元素的个数
		//ll.size();
		System.out.println("指定元素个数是"+ll.size());
		//迭代器  遍历
		Iterator it = ll.iterator();
		while(it.hasNext()){
				System.out.println(it.next());
		}
	
		for(String s:ll){
			System.out.println(s);
		}
		
		
		
	}
}