import java.util.*;
public class HashMap{
	public static void main(String [] args){
		//创建HashMap集合
		Map<String,String> hm = new HashMap<String,String>();
		//添加映射
		hm.put("1","xiaoming");
		hm.put("2","xiaoqiang");
		hm.put("3","xiaopang");
		hm.put("4","xiaofeng");
		//移除指定映射
		hm.remove("3");
		//移除所有映射
		//hm.clear();
		//查找映射指定值
		//hm.get(3);
		System.out.println(hm.get(3));
	}
}